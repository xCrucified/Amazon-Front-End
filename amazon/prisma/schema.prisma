generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AspNetUsers {
  Id             String        @id 
  UserName       String     @unique
  PasswordHash   String
  Email          String     @unique
  BirthDate      DateTime
  PhoneNumber    String 
  AvatarPicture  String?
}