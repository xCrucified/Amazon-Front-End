generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int        @id @default(autoincrement())
  email          String     @unique
  username       String?    @unique
  avatarPicture  String?
  password       String
  phoneNumber    String?    @unique
  createdAt      DateTime   @default(now())
  birthDate      DateTime?
}