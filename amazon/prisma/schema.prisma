generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int        @id @default(autoincrement())
  username       String     @unique
  passwordHash   String
  email          String     @unique
  birthDate      DateTime
  countryCode    Int
  phoneNumber    String 
  avatarPicture  String?
}